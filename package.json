{
  "name": "@azoom/nnn-router",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azoom/nnn-router"
  },
  "author": "Azoom",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/declaration/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "default": "./dist/cjs/index.js"
  },
  "scripts": {
    "prepublish": "yarn build",
    "prebuild": "yarn test && rimraf dist",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig-esm.json && renamer --find '/\\.js$/' --replace '.mjs' 'dist/esm/**/*.js'",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --no-cache -i"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "express": "^4.0.0",
    "glob": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.0.0",
    "@types/glob": "^7.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^14.0.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.6.0",
    "node-mocks-http": "^1.12.1",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "volta": {
    "node": "14.21.2",
    "yarn": "1.22.19"
  }
}
